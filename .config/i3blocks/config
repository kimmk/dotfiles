# i3blocks config file
#
# Please see man i3blocks for a complete reference!
# The man page is also hosted at http://vivien.github.io/i3blocks
#
# List of valid properties:
#
# align
# color
# command
# full_text
# instance
# interval
# label
# min_width
# name
# separator
# separator_block_width
# short_text
# signal
# urgent

markup=pango

[net]
command=~/.config/i3blocks/net 2
interval=2

[disk]
command=~/.config/i3blocks/disk
interval=30

[mem]
command=~/.config/i3blocks/mem
interval=5

[cpu]
command=~/.config/i3blocks/cpu 5
interval=5

[bat]
command=~/.config/i3blocks/bat
interval=30

[time_utc_0]
command=~/.config/i3blocks/utc
interval=1

[time]
command=~/.config/i3blocks/date
interval=1

